{
  "name": "file-to-datauri",
  "description": "Convert local files into data uris with ease.",
  "repository": "https://github.com/parro-it/file-to-datauri.git",
  "bugs": {
    "url": "https://github.com/parro-it/file-to-datauri/issues"
  },
  "keywords": [
    "data",
    "uri",
    "data-uri",
    "file",
    "files",
    "sync",
    "async"
  ],
  "version": "1.0.1",
  "main": "dist/index.js",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "mime": "^1.3.4"
  },
  "author": "andrea@parro.it",
  "scripts": {
    "test": "mocha test/*_test.js test/**/*_test.js --require ./test/_init || echo failed",
    "test-release": "export TEST_RELEASE=1 && mocha dist_test/*_test.js dist_test/**/*_test.js --require ./dist_test/_init-release",
    "build": "babel es6 --out-dir dist",
    "build-test": "babel test --out-dir dist_test && cp -r test/fixture dist_test",
    "lint": "eslint es6/*.js test/*.js",
    "check": "npm run lint && npm run build && npm run build-test && npm run test-release",
    "prepublish": "npm run check"
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb",
    "plugins": [
      "babel"
    ]
  },
  "babel": {
    "stage": 0,
    "optional": [
      "runtime"
    ]
  },
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-eslint": "^4.0.5",
    "chai": "^2.2.0",
    "co": "^4.5.2",
    "co-mocha": "^1.1.0",
    "eslint": "^0.24.1",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-babel": "^1.2.0",
    "mocha": "^2.2.4",
    "thenify": "^3.1.0"
  },
  "license": "MIT"
}
